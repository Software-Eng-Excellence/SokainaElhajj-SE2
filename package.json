  "scripts": {
    "lint": "eslint src",
    "clean": "rimraf build",
    "prebuild": "npm run clean && npm run lint",
    "build": "tsc",
    "postbuild": "node -e \"const fs=require('fs');const getAllFiles=d=>fs.readdirSync(d).reduce((acc,f)=>{const p=require('path').join(d,f);return acc.concat(fs.statSync(p).isDirectory()?getAllFiles(p):p)},[]);const size=getAllFiles('build').reduce((s,f)=>s+fs.statSync(f).size,0);console.log('Finished build with size: ' + (size/1024/1024).toFixed(2) + ' MB')\"",
    "start": "node build/src/index.js",
    "dev": "ts-node-dev --respawn src/index.ts",
    "test": "jest"
  },
